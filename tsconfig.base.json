{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "lib": ["ES2021"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@meos/launcher-components/*": ["../../packages/launcher-components/dist/*"],
      "@meos/launcher-core/*": ["../../packages/launcher-core/*"]
    }
  }
}
